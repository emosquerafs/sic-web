{
  "printWidth": 150,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "trailingComma": "none",
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "singleAttributePerLine": false,
  "jsxBracketSameLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf"
}
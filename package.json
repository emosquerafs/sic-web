{
  "name": "poseidon-web",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build",
    "build:dev": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=development --aot",
    "build:prod": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=production --aot",
    "build:qa": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=qa --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --sourceMap=false --browsers=ChromeHeadless --watch=false",
    "format:check": "prettier --check \"**/src/**/*.{ts,js,json,html,css,scss,md}\"",
    "format": "prettier --write \"**/src/**/*.{ts,html,scss,css,json}\" \"src/styles.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
    "@fullcalendar/angular": "^6.1.16",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "keycloak-angular": "^15.3.0",
    "keycloak-js": "^24.0.5",
    "ngx-translate-multi-http-loader": "^15.0.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.15",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular/cli": "^17.3.11",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "3.4.2",
    "typescript": "~5.4.2"
  }
}
